{
  "name": "AWS CDK Project MCP Configuration",
  "mcpServers": {
    "awslabs.core-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.core-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AWS_REGION": "ap-northeast-1"
      },
      "disabled": false
    },
    "awslabs.cdk-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.cdk-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AWS_REGION": "ap-northeast-1",
        "CDK_DEFAULT_REGION": "ap-northeast-1"
      },
      "disabled": false
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/ubuntu"
      ],
      "env": {},
      "disabled": false
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {},
      "disabled": false
    },
    "awslabs.aws-documentation-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.aws-documentation-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR",
        "AWS_DOCUMENTATION_PARTITION": "aws"
      },
      "disabled": false
    },
    "mcp-server-bigquery": {
      "command": "uvx",
      "args": [
        "mcp-server-bigquery@latest"
      ],
      "env": {},
      "disabled": false
    },
    "o3-search-mcp": {
      "command": "npx",
      "args": [
        "o3-search-mcp"
      ],
      "env": {
        "SEARCH_CONTEXT_SIZE": "medium",
        "REASONING_EFFORT": "medium"
      },
      "disabled": false
    },
    "mcp-server-git": {
      "command": "uvx",
      "args": [
        "--directory",
        "/home/ubuntu/workspace",
        "mcp-server-git@latest"
      ],
      "env": {},
      "disabled": false
    },
    "deep-research": {
      "command": "uvx",
      "args": [
        "mcp-server-deepresearch",
        "--openai_api_key",
        "${OPENAI_API_KEY}"
      ],
      "env": {},
      "disabled": false
    },
    "knowledge-graph": {
      "command": "npx",
      "args": [
        "mcp-knowledge-graph",
        "--memory-path",
        "/home/ubuntu/workspace/cic-proxy/memory.jsonl"
      ],
      "env": {},
      "disabled": false
    },
    "pymupdf4llm-mcp": {
      "command": "uvx",
      "args": [
        "pymupdf4llm-mcp@latest",
        "stdio"
      ],
      "env": {},
      "disabled": false
    },
    "pdf-tools": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/hanweg/mcp-pdf-tools.git",
        "pdf-tools"
      ],
      "env": {},
      "disabled": false
    },
    "mcp-ocr": {
      "command": "uvx",
      "args": [
        "mcp-ocr"
      ],
      "env": {},
      "disabled": false
    }
  }
}
